elementor-element elementor-element-967eab9 elementor-widget elementor-widget-html\" data-id=\"967eab9\" data-element_type=\"widget\" data-settings=\"{&quot;ekit_we_effect_on&quot;:&quot;none&quot;}\" data-widget_type=\"html.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t<script>\n  jQuery(document).ready(function($) {\n    const sections = $('.all-sections-heading');\n    const navLinks = $('.custom-sidebar a');\n\n    function activateLink(link) {\n      navLinks.removeClass('active-item');\n      link.addClass('active-item');\n    }\n\n    \/\/ On click, change the active class and scroll to section with offset\n    navLinks.on('click', function(e) {\n      e.preventDefault();\n      const targetId = $(this).attr('href');\n      activateLink($(this));\n      $('html, body').animate({\n        scrollTop: $(targetId).offset().top - 80\n      }, 300);\n    });\n\n    \/\/ On scroll, change the active class dynamically\n    $(window).on('scroll', function() {\n      let currentSection = '';\n\n      sections.each(function() {\n        const sectionTop = $(this).offset().top;\n        const sectionHeight = $(this).outerHeight();\n        if ($(window).scrollTop() >= sectionTop - sectionHeight \/ 3 - 80) {\n          currentSection = $(this).attr('id');\n        }\n      });\n\n      navLinks.each(function() {\n        if ($(this).attr('href') === '#' + currentSection) {\n          activateLink($(this));\n        }\n      });\n    });\n  });\n<\/script>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t<\/section>\n\t\t\t\t\t<\/div>\n\t\t<\/div>\n\t\t\t\t<div class=\"elementor-column elementor-col-33 elementor-top-column elementor-element elementor-element-658756f\" data-id=\"658756f\" data-element_type=\"column\">\n\t\t\t<div class=\"elementor-widget-wrap elementor-element-populated\">\n\t\t\t\t\t\t<div class=\"elementor-element elementor-element-79de511 elementor-widget elementor-widget-slide-compact-