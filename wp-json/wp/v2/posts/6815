ionTop = $(this).offset().top;\n        const sectionHeight = $(this).outerHeight();\n        if ($(window).scrollTop() >= sectionTop - sectionHeight \/ 3 - 80) {\n          currentSection = $(this).attr('id');\n        }\n      });\n\n      navLinks.each(function() {\n        if ($(this).attr('href') === '#' + currentSection) {\n          activateLink($(this));\n        }\n      });\n    });\n  });\n<\/script>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t<\/section>\n\t\t\t\t\t<\/div>\n\t\t<\/div>\n\t\t\t\t<div class=\"elementor-column elementor-col-33 elementor-top-column elementor-element elementor-element-30408a8\" data-id=\"30408a8\" data-element_type=\"column\">\n\t\t\t<div class=\"elementor-widget-wrap elementor-element-populated\">\n\t\t\t\t\t\t<div class=\"elementor-element elementor-element-16ece85 elementor-widget elementor-widget-slide-compact-player\" data-id=\"16ece85\" data-element_type=\"widget\" data-settings=\"{&quot;ekit_we_effect_on&quot;:&quot;none&quot;}\" data-widget_type=\"slide-compact-player.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t\r\n\t\t<div class=\"swp_music_player compact-player\" id=\"mpfe_697117bdde73b\" data-autoplay=\"no\" data-playmode=\"repeat\" data-stopplaylistend=\"no\" data-stopsongend=\"no\" data-repeatmode=\"unset\">\r\n\t\t\t<div class=\"swp-compact-player\">\r\n\t\t\t\t<div class=\"swp-compact-cover\">\r\n\t\t\t\t\t<div class=\"swp-compact-cover-container\"><\/div>\r\n\t\t\t\t\t<div class=\"compact-cover-overlay album_left_overlay lc_swp_overlay\"><\/div>\r\n\t\t\t\t<\/div>\r\n\r\n\t\t\t\t<div class=\"swp-compact-player-info\">\r\n\t\t\t\t\t<div class=\"compact-info-overlay album_right_overlay lc_swp_overlay\"><\/div>\r\n\t\t\t\t\t<div class=\"compact-info-content\">\r\n\t\t\t\t\t\t<div class=\"compact-info-top clearfix\">\r\n\t\t\t\t\t\t\t<div class=\"compact-song-details\">\r\n\t\t\t\t\t\t\t\t<div class=\"current_song_name compact-song-name\">