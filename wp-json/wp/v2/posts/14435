 = $('.custom-sidebar a');\n\n    function activateLink(link) {\n      navLinks.removeClass('active-item');\n      link.addClass('active-item');\n    }\n\n    \/\/ On click, change the active class and scroll to section with offset\n    navLinks.on('click', function(e) {\n      e.preventDefault();\n      const targetId = $(this).attr('href');\n      activateLink($(this));\n      $('html, body').animate({\n        scrollTop: $(targetId).offset().top - 80\n      }, 300);\n    });\n\n    \/\/ On scroll, change the active class dynamically\n    $(window).on('scroll', function() {\n      let currentSection = '';\n\n      sections.each(function() {\n        const sectionTop = $(this).offset().top;\n        const sectionHeight = $(this).outerHeight();\n        if ($(window).scrollTop() >= sectionTop - sectionHeight \/ 3 - 80) {\n          currentSection = $(this).attr('id');\n        }\n      });\n\n      navLinks.each(function() {\n        if ($(this).attr('href') === '#' + currentSection) {\n          activateLink($(this));\n        }\n      });\n    });\n  });\n<\/script>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t<\/section>\n\t\t\t\t\t<\/div>\n\t\t<\/div>\n\t\t\t\t<div class=\"elementor-column elementor-col-33 elementor-top-column elementor-element elementor-element-58b3e3d\" data-id=\"58b3e3d\" data-element_type=\"column\">\n\t\t\t<div class=\"elementor-widget-wrap elementor-element-populated\">\n\t\t\t\t\t\t<div class=\"elementor-element elementor-element-1719138 elementor-widget elementor-widget-slide-compact-player\" data-id=\"1719138\" data-element_type=\"widget\" data-settings=\"{&quot;ekit_we_effect_on&quot;:&quot;none&quot;}\" data-widget_type=\"slide-compact-player.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t\r\n\t\t<div class=\"swp_music_player compact-player\" id=\"mpfe_69711a57e2f93\" data-autoplay=\"no\" data-playmode=\"repeat\" data-stopplaylistend=\"no\" data-stopsongend=\"no\"